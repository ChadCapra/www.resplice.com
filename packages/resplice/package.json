{
  "name": "resplice",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "lint": "prettier --check . && eslint --ignore-path .gitignore .",
    "format": "prettier --write .",
    "proto": "pbjs -t static-module --no-service --no-beautify -w es6 -o src/services/resplice-pb.js  ../../../proto/*.proto && pbts -o src/services/resplice-pb.d.ts src/services/resplice-pb.js",
    "proto:debug": "pbjs -t static-module --no-service -w es6 -o src/services/resplice-pb.js ../../../proto/*.proto  && pbts -o src/services/resplice-pb.d.ts src/services/resplice-pb.js",
    "test": "jest",
    "test:e2e": "jest -c jest.e2e.config.cjs"
  },
  "devDependencies": {
    "@dicebear/avatars": "^4.10.2",
    "@dicebear/avatars-gridy-sprites": "^4.10.2",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-static": "1.0.0-next.24",
    "@sveltejs/kit": "^1.0.0-next.212",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@testing-library/svelte": "^3.0.3",
    "@types/croppie": "^2.6.1",
    "@types/jest": "^27.4.0",
    "@types/qrcode": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.1",
    "classnames": "^2.3.1",
    "croppie": "^2.6.5",
    "cssnano": "^5.0.14",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.4.5",
    "jest-playwright-preset": "^1.7.0",
    "jsqr": "^1.4.0",
    "libphonenumber-js": "^1.9.44",
    "playwright": "^1.17.1",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "protobufjs": "^6.11.2",
    "qrcode": "^1.5.0",
    "svelte": "^3.44.3",
    "svelte-i18n": "^3.3.13",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.8",
    "ts-jest": "^27.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.10"
  },
  "type": "module",
  "engines": {
    "node": ">= 12.17.0"
  }
}
